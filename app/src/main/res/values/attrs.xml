<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Navigator">
        <attr name="checked" format="boolean"/>
        <attr name="icon" format="reference"/>
        <attr name="text" format="string"/>
        <attr name="title" format="string"/>
    </declare-styleable>

    <declare-styleable name="Navigator.SettingsItem" parent="Navigator">
        <attr name="checked"/>
        <attr name="text"/>
    </declare-styleable>

    <declare-styleable name="Navigator.MenuItem" parent="Navigator">
        <attr name="text"/>
        <attr name="icon"/>
    </declare-styleable>

    <declare-styleable name="Navigator.Toolbar" parent="Navigator">
        <attr name="title"/>
        <attr name="icon"/>
    </declare-styleable>
</resources>
